{
  "name": "maineffectjs",
  "version": "0.3.0",
  "description": "Testing in javascript made easier.",
  "main": "dist/maineffect.js",
  "scripts": {
    "test": "jest --coverage",
    "test-mocha": "nyc mocha src/**/*.test.* --require ./babel-register.js --require global-jsdom/register",
    "build": "npx webpack --config webpack.config.js"
  },
  "keywords": [
    "unit",
    "test",
    "mock",
    "left"
  ],
  "author": "arvind.raj.naidu@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.4.0",
    "@babel/traverse": "^7.8.4",    
    "babel-loader": "^8.0.5"    
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "global-jsdom": "^8.6.0",
    "jest-environment-jsdom": "^29.2.2",
    "jsdom": "^20.0.1",
    "mocha": "^7.0.1",
    "nodemon": "1.12.0",
    "nyc": "^13.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.0.0",
    "sinon": "^7.4.1",
    "@testing-library/react": "^13.4.0",    
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/examples/**/*.js"
    ],
    "exclude": [
      "**/**.test.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arvindrajnaidu/maineffect.git"
  },
  "bugs": {
    "url": "https://github.com/arvindrajnaidu/maineffect/issues"
  },
  "homepage": "https://github.com/arvindrajnaidu/maineffect#readme"
}
